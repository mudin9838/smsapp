                @Html.DisplayFor(modelItem => item.Vat)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TotalPrice)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.OutDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Department.DepartmentId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Employee.EmployeeId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Parent.ParentId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Status.StatusId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Statusdel.StatusdelId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Stock.StockId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.OutId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.OutId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.OutId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
